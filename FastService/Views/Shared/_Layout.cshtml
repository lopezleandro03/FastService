<script>
    $(function () {
        $.ajax({
            type: 'GET',
            url: "/@Model.DefaultController/@Model.DefaultAction",
            contentType: "html",
            dataType: "html",
            success: function (data) {
                $(".page-content").html(data);
            },
            error: function (data) {
                alert("Oops, este modulo no ha sido implementado aun...");
            }
        });
    });

    $(".asyncAction").click(function () {
        $.ajax({
            type: 'GET',
            url: "/" + $(this).data("controller") + "/" + $(this).data("action"),
            contentType: "html",
            dataType: "html",
            success: function (data) {
                $(".page-content").html(data);
            },
            error: function (data) {
                alert("Oops, este modulo no ha sido implementado...");
            }
        });

    });

    function OnSuccess(result) {
        if (result.Success == "true") {
            $("#modalSuccess").modal();
            $.ajax({
                type: 'GET',
                url: "/Venta/Create",
                contentType: "html",
                dataType: "html",
                success: function (data) {

                    $(".page-content").html(data);
                },
                error: function (data) {
                    $("#modalError").modal();
                    //alert("Oops, ocurrio un error procesando su solicitud...");
                }
            });
        } else {
            $("#modalError").modal();
            //alert("Oops, ocurrio un error procesando su solicitud...");
        }
    }

    function OnError(result) {
        //alert("Oops, ha ocurrido un error procesando su solicitud...");
        //$(".page-content").html(data);
        $("#modalError").modal();
    }
</script>
