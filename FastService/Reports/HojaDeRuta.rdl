r /></TablixMembers></TablixMember></TablixMembers></TablixRowHierarchy><DataSetName>tickets</DataSetName><Top>0.59944in</Top><Left>0.16194in</Left><Height>1.54514in</Height><Width>7.83806in</Width><ZIndex>1</ZIndex><Style><Border><Style>None</Style></Border></Style></Tablix></ReportItems><Height>6.83806in</Height><Style /></Body><Width>8in</Width><Page><PageHeight>11.69in</PageHeight><PageWidth>8.27in</PageWidth><LeftMargin>0.1in</LeftMargin><RightMargin>0.1in</RightMargin><TopMargin>0.1in</TopMargin><BottomMargin>0.1in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="FastServiceDatabase"><DataSourceReference>FastServiceDatabase</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>9ebd36ab-2cc6-43dd-a071-587e2b6c4492</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="tickets"><Query><DataSourceName>FastServiceDatabase</DataSourceName><CommandText>use FastService

select top 15 
r.ReparacionId as Ticket,
(case rd.EsGarantia when 1 then 'E' else 'C' end) as Garantia,
c.Nombre + c.Apellido as Nombre, 
c.Telefono1 as Telefonos, 
rd.Modelo, 
rd.Serie, 
m.nombre as Marca, 
c.Direccion, 
d.Calle, 
d.Altura, 
d.Calle2, 
d.Calle3, 
d.Ciudad, 
d.CodigoPostal
from Reparacion r
join Cliente c on r.ClienteId = c.ClienteId
join ReparacionDetalle rd on r.ReparacionDetalleId = rd.ReparacionDetalleId
join Marca m on r.MarcaId = m.Marcaid
join Direccion d on c.DireccionId = d.Direccionid
order by 1 desc</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="Ticket"><DataField>Ticket</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Garantia"><DataField>Garantia</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Nombre"><DataField>Nombre</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Telefonos"><DataField>Telefonos</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Modelo"><DataField>Modelo</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Serie"><DataField>Serie</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Marca"><DataField>Marca</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Direccion"><DataField>Direccion</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Calle"><DataField>Calle</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Altura"><DataField>Altura</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Calle2"><DataField>Calle2</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Calle3"><DataField>Calle3</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Ciudad"><DataField>Ciudad</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CodigoPostal"><DataField>CodigoPostal</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><Language>es-ES</Language><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType>Inch</rd:ReportUnitType><rd:ReportID>fa26d337-042a-4022-98fa-6e44e5710e2f</rd:ReportID></Report>